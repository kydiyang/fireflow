<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-update="true" name="org.fireflow.engine.definition.WorkflowDefinition" table="T_FF_DF_WORKFLOWDEF">
    <id name="id" type="java.lang.String">
      <column length="50" name="ID"/>
      <generator class="uuid.hex">
            </generator>
    </id>
    <property name="processId" type="java.lang.String">
      <column length="100" name="PROCESS_ID" not-null="true"/>
    </property>
    <property name="name" type="java.lang.String">
      <column length="100" name="NAME" not-null="true"/>
    </property>
    <property name="displayName" type="java.lang.String">
      <column length="128" name="DISPLAY_NAME"/>
    </property>
    <property name="description" type="java.lang.String">
      <column length="1024" name="DESCRIPTION"/>
    </property>
    <property name="version" type="java.lang.Integer">
      <column length="4" name="VERSION" not-null="true"/>
    </property>
    <property name="processContent" type="org.springframework.orm.hibernate3.support.ClobStringType">
      <column name="PROCESS_CONTENT"/>
    </property>
    <property name="published" type="java.lang.Boolean">
      <column length="1" name="PUBLISHED"/>
    </property>
    <property name="publisher" type="java.lang.String">
      <column length="50" name="PUBLISHER"/>
    </property>
    <property name="publishTime" type="java.util.Date">
      <column name="PUBLISH_TIME"/>
    </property>
  </class>
</hibernate-mapping>
